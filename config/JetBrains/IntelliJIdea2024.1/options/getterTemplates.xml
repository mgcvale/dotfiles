<application>
  <component name="GetterTemplates" defaultTemplateName="IntelliJ Default">
    <option name="templates">
      <list>
        <TemplateResource>
          <option name="fileName" value="fieldValueGetter" />
          <option name="template" value="#set($name = $StringUtil.capitalizeWithJavaBeanConvention($StringUtil.sanitizeJavaIdentifier($helper.getPropertyName($field, $project))))&#10;public String get${name}() {&#10;return ${field.name}.getText();&#10;}" />
        </TemplateResource>
      </list>
    </option>
  </component>
</application>